#include <iostream>
int max(int num1 = 1, int num2 = 1); 
int main() {
    int a, b;
    std :: cout << "Enter two numbers: " << "\n";
    std :: cin >> a >> b; 
    int k = max(a, b); 
    std :: cout << "Max = " << k << "\n"; 
    return 0;
}
int max(int num1, int num2) { 
    int maximum; 
    if (num1 > num2){
        maximum = num1;
    } else{
        maximum = num2;
    }
    return maximum;
}
